pipeline {
    agent any
    tools {
        maven 'Maven 3.6.3'
        jdk 'jdk8'
    }
    stages {
        stage ('Initialize') {
          steps {
              sh '''
                  echo "Initialize.."
                  echo "PATH = ${PATH}"
                  echo "M2_HOME = ${M2_HOME}"
              '''
          }
        }
        stage('Compile') {
          steps {
            echo 'Compile..'
            sh 'mvn clean compile'
          }
        }
        stage('Test') {
          echo 'Test..'
          sh 'mvn test'
        }
    }
}
